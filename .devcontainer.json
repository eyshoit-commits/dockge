{
  "name": "Dockge DevContainer",
  "build": {
    "dockerComposeFile": "compose.yaml",
    "target": "development"
  },
  "customizations": {
    "vscode": {
      "extensions": ["ms-azuretools.vscode-docker"]
    }
  },
  "mounts": [
    "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
  ],
  "remoteUser": "node"
}

